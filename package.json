{
  "name": "rylie-seo-hub",
  "version": "1.0.0",
  "description": "AI-Powered SEO Assistant for Automotive Dealerships",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:setup": "chmod +x scripts/migrate-database.sh && ./scripts/migrate-database.sh",
    "test": "jest",
    "onboarding:deploy": "npm run db:setup && npm run build",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "setup-user-agency": "tsx scripts/setup-user-agency.ts",
    "make-super-admin": "tsx scripts/make-super-admin.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^9.30.0",
    "@types/node-cron": "^3.0.11",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "cron-parser": "^5.3.0",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "handlebars": "^4.7.8",
    "html-pdf-node": "^1.0.8",
    "lucide-react": "^0.522.0",
    "next": "^15.3.4",
    "next-auth": "5.0.0-beta.25",
    "node-cron": "^4.1.1",
    "nodemailer": "^6.10.1",
    "openai": "^5.6.0",
    "posthog-js": "^1.255.1",
    "puppeteer": "^24.10.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/gapi": "^0.0.47",
    "@types/google-one-tap": "^1.2.6",
    "@types/jest": "^30.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.5.3",
    "prisma": "^6.10.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/copp1723/rylie-seo-hub.git"
  },
  "keywords": [
    "seo",
    "automotive",
    "ai",
    "dealership",
    "white-label",
    "saas",
    "nextjs",
    "typescript",
    "prisma",
    "tailwindcss"
  ],
  "author": "SEO Werks",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}